<div class="page-header">
  <h1>
    Travelers
    <a href="/travelers/new" class="btn btn-lg btn-success">New Traveler</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Date of travel</th>
        <th>Origin</th>
        <th>Destination</th>
        <th>Acceptable weight lbs</th>
        <th>Expected fee</th>
        <th>User</th>
        <th>Actions</th>
      </tr>

      <% @travelers.each do |traveler| %>
      <tr>
        <td><%= traveler.date_of_travel %></td>
        <td><%= traveler.origin_id %></td>
        <td><%= traveler.destination_id %></td>
        <td><%= traveler.acceptable_weight_lbs %></td>
        <td><%= traveler.expected_fee %></td>
        <td><%= traveler.user_id %></td>
        <td>
          <a href="/travelers/<%= traveler.id %>" class="btn btn-primary">Show</a>
          <a href="/travelers/<%= traveler.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_traveler/<%= traveler.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
